class Solution {
public:
    vector<string> validStrings(int n) {
        vector<string> ans;
        int m = 1 << n;

        for (int num = 0; num < m; num++) {
            string s = "";
            for (int i = n - 1; i >= 0; i--) {
                if (num & (1 << i))
                    s += '1';
                else
                    s += '0';
            }
            ans.push_back(s);
        }
        return ans;
    }
};
