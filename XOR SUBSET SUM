class Solution {
public:
    int subsetXORSum(vector<int>& nums) {
        int m = nums.size();
        int n = 1 << m;
        int sum = 0;

        for (int mask = 0; mask < n; mask++) {
            int x = 0;
            for (int j = 0; j < m; j++) {
                if (mask & (1 << j)) {
                    x ^= nums[j];
                }
            }
            sum += x;
        }
        return sum;
    }
};
