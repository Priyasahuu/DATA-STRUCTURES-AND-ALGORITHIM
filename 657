class Solution {
public:
    bool judgeCircle(string moves) {
        pair< int , int> p={0 , 0};
        int n =moves.size();
        for( int i = 0 ; i < n ; i ++){
            if(moves[i]=='D'){
                p.first -=1;

            }
          else if(moves[i]=='U') p.first +=1;
          else if( moves[i]=='L')p.second -=1;
          else{
            p.second +=1;
          }
        }
        if(p.first==0 &&p.second==0)return true;
        return false;
    }
};
