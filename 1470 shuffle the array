class Solution {
public:
    vector<int> shuffle(vector<int>& nums, int n) {
      vector<int> ans;
    int i =0 ;  
    int j = n ;
    int flag=true;
    while( i < n || j >2*n ){
        if( flag){
            if( i < n)ans.push_back( nums[i]);
            flag = !flag;
            i+=1;
        }
        else{
            if( j < 2*n)ans.push_back(nums[j]);
            j+=1;
            flag= !flag;
        }
    }
    ans.push_back( nums[2*n -1]);
    return ans;
    }
}; 
tc=O(N)
sc=O(N)
