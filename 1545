class Solution {
public:
   void f(string &s1){
    string temp=s1;
    s1+='1';
   
   for( int i = temp.size()-1; i>=0 ; i --){
    if( temp[i]=='0')s1+='1';
    else s1+='0';
   }
   }
    char findKthBit(int n, int k) {
       string s1="0";
       for( int i = 1 ; i  < n ; i ++){
        f(s1);
       } 
       return s1[k-1];
    }
};
