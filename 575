class Solution {
public:
    int distributeCandies(vector<int>& candyType) {
     int n = candyType.size();
     unordered_map< int  , int>mpp;
     for( int i = 0 ; i < n ;  i ++){
        mpp[candyType[i]]+=1;
     }   
     int m = mpp.size();
     int num = n/2;
     if(num > m)return m;
     return num;
    }
};
