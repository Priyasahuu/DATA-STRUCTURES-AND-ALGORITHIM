class Solution {
public:
    int mostWordsFound(vector<string>& sentences) {
        int maxi=0;
       for( auto it: sentences){
        stringstream ss(it);
        string word;
        int cnt=0;
        while(ss>>word){
            cnt+=1;
        }
        maxi= max( cnt , maxi);
       } 
       return maxi;
    }
};
