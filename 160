/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    ListNode *getIntersectionNode(ListNode *headA, ListNode *headB) {
        unordered_map<ListNode* , bool> mpp;
    
       while(headA ){
        mpp[headA]=true;
        headA =headA-> next;

       }
       while(headB){
        if( mpp.find(headB) !=mpp.end())return headB;
        headB= headB-> next;
       }
         return nullptr;
    }
};
