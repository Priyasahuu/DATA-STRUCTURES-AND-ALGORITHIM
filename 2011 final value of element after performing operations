class Solution {
public:
    int finalValueAfterOperations(vector<string>& operations) {
        int i =0;
        int n = operations.size();
        for( int j = 0 ;  j  < n ; j ++){
            if( operations[j]=="--X")i-=1;
            else if( operations[j]=="X--") i= -1+i;
           else i+=1;
        }
        return i;
    }
};
